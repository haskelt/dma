					"10" : "F (0.0-0.8)",
					"11" : "S (satisfactory)",
					"12" : "U (unsatisfactory)",
					"13" : "Prefer not to answer"
				},
				"age" : {
					"0" : "17 or younger",
					"1" : "18-19",
					"2" : "20-22",
					"3" : "23-29",
