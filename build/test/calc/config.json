		"pretest1" : {
			"class" : "AutoData"
		},
		"pretest2" : {
			"class" : "AutoData"
		},
		"pretest3" : {
			"class" : "AutoData"
		},
		"test1" : {
			"class" : "AutoData"
		},
		"test2" : {
			"class" : "AutoData"
		},
		"test3" : {
			"class" : "AutoData"
		},
		"psvt" : {
			"class" : "AutoData",
			"headerMappings" : {
				"Name" : "Name",
				"Pre-Course Assessment" : "psvt_pre",
			    "Post-Course Assessment" : "psvt_post",
			    "Visualization of Rotations TAKE 2": "psvt_post",
			    "Visualization of Rotations": "psvt_pre"
			}
		}
	},
