class ErrorRecovery {
    
    /**************************************************************************/

    static setResumePoint (f) {

	this.resumePoint = f;

    } // setResumePoint

    /**************************************************************************/

    static resume () {

	this.resumePoint();
	
    } // resume

    /**************************************************************************/

}

export default ErrorRecovery;
